{
  "private": "true",
	"type": "module",
  "scripts": {
    "dev:ui": "pnpm --filter nocturnal-ui dev",
    "build:ui": "pnpm --filter nocturnal-ui build",
    "tsc:ui": "pnpm --filter nocturnal-ui tsc",
    "test:ui": "pnpm --filter nocturnal-ui test",
    "lint:ui": "pnpm --filter nocturnal-ui lint",
    "format:ui": "pnpm --filter nocturnal-ui format",
    "prepare": "husky install",
    "gen-types:ui": "pnpm --filter nocturnal-ui gen:types",
    "gen-scss:ui": "pnpm --filter nocturnal-ui gen:scss",
		"rm-declaration:ui": "pnpm --filter nocturnal-ui remove:declaration",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:serve": "pnpm --filter docs serve"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vue/test-utils": "^2.2.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "eslint-plugin-vue": "^9.6.0",
    "esno": "^0.16.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.4",
    "plop": "^3.1.1",
    "prettier": "2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-executor": "workspace:*",
    "vitest": "^0.24.3",
    "vue-tsc": "1.0.9"
  },
  "dependenciesMeta.*.injected": true,
  "author": "",
  "license": "MIT",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "@algolia/client-search"
      ]
    }
  }
}
